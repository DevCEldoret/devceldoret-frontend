    "eslint-config-ts-react-important-stuff": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "file-loader": "^5.0.2",
    "gh-pages": "^2.1.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-cli": "^24.9.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "style-loader": "^1.0.0",
    "typescript": "^3.7.4",
    "webpack": "^4.41.4",
